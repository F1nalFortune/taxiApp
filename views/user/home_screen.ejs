    <div class="window">
      <% include ../partials/navbar %>
      <div class="content">
        <input id="searchTextField" type="text" size="50" placeholder="Where to?">
        <div id="map"></div>
        <div id="confirmation">
        </div>
      </div>
    </div>
  </body>
  <script type="text/javascript" src="js/home_screen.js"></script>
