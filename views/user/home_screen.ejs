      #map {
        height: 100% !important;
        margin: 0 !important;
        padding: 0 !important;
      }
      /* Optional: Makes the sample page fill the window. */
      html, body {
        height: 100%;
        margin: 0;
        padding: 0;
      }
    </style>
  </head>
